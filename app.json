{
  "id": "webhook.manager",
  "version": "0.1.0",
  "compatibility": ">=0.0.0",
  "category": "internet",
  "name": {
    "en": "Webhook Manager",
    "nl": "Webhook Manager"
  },
  "description": {
    "en": "Add external values to Homey webhooks",
    "nl": "Voeg waardes toe aan Homey vanuit externe webhooks"
  },
  "author": {
    "name": "Chamid Media",
    "email": "info@chamidmedia.nl"
  },
  "images": {
    "small": "assets/images/small.png",
    "large": "assets/images/large.png"
  },
  "permissions": [
  ],
  "speech": [
  ],
  "flow": {
    "triggers": [
      {
        "id": "event",
        "title": {
          "en": "On event",
          "nl": "Als triggered"
        },
        "args": [
          {
            "name": "event",
            "type": "text",
            "placeholder": {
              "en": "Event name",
              "nl": "Event naam"
            }
          }
        ],
        "tokens": [
            {
                "name": "data1",
                "title": {
                    "en": "data1",
                    "nl": "data1"
                }
            },
            {
                "name": "data2",
                "title": {
                    "en": "data2",
                    "nl": "data2"
                }
            },
            {
                "name": "data3",
                "title": {
                    "en": "data3",
                    "nl": "data3"
                }
            }
        ]
      }
    ]
  }
}